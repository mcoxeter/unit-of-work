{
  "schema": [
    {
      "name": "persons.[].forename"
    },
    {
      "name": "persons.[].surname",
      "validation": ["TWO_OR_MORE"],
      "manditory": true
    },
    {
      "name": "persons.[].age",
      "validation": ["GREATER_THAN_ZERO", "LESS_THAN_ONEHUNDRED"]
    },
    {
      "name": "persons.[].dob",
      "synced": true
    },
    {
      "name": "persons.[].addresses",
      "ordering": "manual"
    },
    {
      "name": "persons.[].addresses.[].street1",
      "validation": ["NOT_EMPTY"],
      "manditory": true
    },
    {
      "name": "persons.[].addresses.[].country",
      "lookup": "country-choices",
      "ordering": "alpha.desc"
    }
  ]
}
